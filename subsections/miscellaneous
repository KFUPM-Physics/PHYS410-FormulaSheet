\subsection*{\underline{Miscellaneous:}}
\begin{align}
    \int_{-\infty}^\infty e^{-ax^2}\, dx=\sqrt{\frac{\pi}{a}};
     &  & 
    \int_{-\infty}^\infty e^{-(ax^2+bx)}\,dx=e^{b^2/4a}\sqrt{\frac{\pi}{a}};
     &  & 
    \delta_{ij} =
    \begin{cases}
        1, & i=j     \\
        0, & i\neq j
    \end{cases}
\end{align}